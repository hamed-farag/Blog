{
  "name": "hamed-farag-blog",
  "description": "Hamed's Personal Blog Stuffed with Code",
  "version": "1.0.0",
  "author": "Hamed Farag",
  "bugs": {
    "url": "https://github.com/hamed-farag/blog/issues"
  },
  "homepage": "https://hamed-farag.github.io/blog/",
  "keywords": [
    "gatsby",
    "code",
    "personal",
    "blog",
    "react",
    "technical"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hamed-farag/blog.git"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "prebuild": "rmdir /Q /S public && mkdir public",
    "build": "npm run prebuild && gatsby build",
    "build:test": "npm run prebuild && gatsby build && gatsby serve",
    "deploy": "npm run prebuild && gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "disqus-react": "^1.0.5",
    "gatsby": "^2.0.19",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.5",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.6",
    "gatsby-plugin-styled-components": "^3.0.3",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.4",
    "gatsby-remark-prismjs": "^3.0.0",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-remark-smartypants": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.2",
    "gatsby-transformer-remark": "^2.1.6",
    "gatsby-transformer-sharp": "^2.1.3",
    "lodash": "^4.17.11",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-share": "^2.4.0",
    "react-typography": "^0.16.13",
    "styled-components": "^4.1.1",
    "typeface-merriweather": "0.0.43",
    "typeface-montserrat": "0.0.43",
    "typography": "^0.16.17",
    "typography-theme-github": "^0.15.10"
  },
  "devDependencies": {
    "dotenv": "^6.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "prettier": "^1.14.2"
  }
}
